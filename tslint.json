{
  "rulesDirectory": ["tslint-plugin-prettier", "node_modules/tslint-lines-between-class-members"],
  "extends": ["tslint:recommended", "tslint-config-prettier", "tslint-config-airbnb"],
  "rules": {
    "forin": false,
    "interface-name": [true, "never-prefix"],
    "member-ordering": [false],
    "no-console": [false],
    "no-namespace": [true, "allow-declarations"],
    "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
    "quotemark": [true, "single"],
    "only-arrow-functions": true,
    "trailing-comma": false,
    "semicolon": false,
    "object-literal-sort-keys": false,
    "ter-arrow-parens": true,
    "no-increment-decrement": false,
    "brace-style": false,
    "lines-between-class-members": [true, 1],
    "jsdoc-format": false,
    "ordered-imports": false,
    "whitespace": true,
    "triple-equals": true,
    "arrow-return-shorthand": [true, "multiline"],
    "comment-format": [true, "check-space", "check-uppercase", "allow-trailing-lowercase"],
    "class-name": true,
    "completed-docs": [true, "enums", "functions", "methods"],
    "no-unnecessary-initializer": true,
    "no-angle-bracket-type-assertion": true,
    "no-boolean-literal-compare": true,
    "object-literal-key-quotes": [true, "as-needed"],
    "prefer-switch": true,
    "prefer-template": true,
    "align": [true, "parameters", "statements"],
    "eofline": true,
    "no-consecutive-blank-lines": true,
    "no-parameter-reassignment": false,
    "file-header": [
      true,
      {
        "match": "Coded by CallMeKory - https://github.com/callmekory",
        "allow-single-line-comments": true,
        "default": "Coded by CallMeKory - https://github.com/callmekory",
        "enforce-trailing-newline": false
      }
    ]
  }
}
